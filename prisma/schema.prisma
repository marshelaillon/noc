generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_URL")
}

enum SeverityLevel {
  LOW
  MEDIUM
  HIGH
}

model LogModel {
  id Int @id @default(autoincrement())
  message String
  origin String
  level SeverityLevel
  createdAt DateTime @default(now())
}